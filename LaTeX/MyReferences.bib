@article{asikainen_interplay_2018,
	title = {The interplay between academic emotions, psychological flexibility and self-regulation as predictors of academic achievement},
	volume = {42},
	issn = {0309-877X},
	url = {https://search.proquest.com/docview/2024043767},
	doi = {10.1080/0309877X.2017.1281889},
	abstract = {Research concerning students\&\#39; emotions as well as their way of coping with their emotions in different situations has been scarce. The purpose of this study is to explore the relationship between self-regulated learning, academic emotions, psychological flexibility, study success and study pace during university-level studies. A total of 274 arts students participated in this study. Academic emotions were first explored with factor analyses, parallel analysis and confirmatory factor analyses. The relationship between self-regulated learning, psychological flexibility, academic emotions and study success and study pace was analysed using Pearson\&\#39;s correlations and path analysis using Amos. The results revealed that there is a strong connection between self-regulated learning and academic emotions and that students\&\#39; self-regulations appear to affect students\&\#39; emotions with regard to their academic study. Furthermore, psychological flexibility appeared to be an important mediator between academic emotions and study pace. The study suggests that students\&\#39; emotions and their way of coping with these emotions should be taken into account when considering learning and teaching.},
	number = {4},
	journal = {Journal of further and higher education},
	author = {Asikainen, Henna and Hailikari, Telle and Mattsson, Markus},
	year = {2018},
	pages = {439--453},
}

