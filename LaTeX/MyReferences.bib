@article{van_eerde_overcoming_2018,
	title = {Overcoming procrastination? {A} meta-analysis of intervention studies},
	volume = {25},
	issn = {1747-938X},
	url = {http://dx.doi.org/10.1016/j.edurev.2018.09.002},
	doi = {10.1016/j.edurev.2018.09.002},
	abstract = {We present a meta-analysis of 24 studies on procrastination interventions (total k = 44, N = 1173) in order to find out 1) whether people can reduce their level of procrastination, and 2) if so, which type of intervention leads to the strongest reduction. We compared four different types of interventions: Self-regulation, cognitive behavioral therapy, other therapeutic approaches, and interventions focusing on individuals’ strengths and resources. A large reduction in procrastination after the interventions was found, and the effects remained stable in follow-up assessments. The findings so far suggest that cognitive behavioral therapy reduced procrastination more strongly than the other types of interventions. Other moderator variables, such as the duration of the intervention, had no significant effects. We propose future research that may help to build stronger evidence for the effects of interventions, as well as some guidelines for interventions. •Four different types of interventions were included.•A large reduction in procrastination after the interventions was found.•The effects of the interventions remain stable in follow-up assessments.•Cognitive-behavioral therapy reduced procrastination most.•Other moderators (e.g., duration, sample characteristics) had no effects.},
	journal = {Educational research review},
	author = {van Eerde, Wendelien and Klingsieck, Katrin B},
	year = {2018},
	pages = {73--85},
}

